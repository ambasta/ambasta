-<?xml version="1.0" encoding="utf-8"?>
-<section id="reference">
-<title>Variable Reference</title>
-<variablelist>
-  <title>Generating Header Files</title>
-  <varlistentry>
-    <term><parameter>DOC_H_FILE</parameter></term>
-    <listitem><para>
-    The name of the header file to generate
-    </para></listitem>
-  </varlistentry>
-  <varlistentry>
-    <term><parameter>DOC_H_DOCS</parameter></term>
-    <listitem><para>
-    The input DocBook files for generating the header file
-    </para></listitem>
-  </varlistentry>
-</variablelist>
-<variablelist>
-  <title>Generating Documentation Files</title>
-  <varlistentry>
-    <term><parameter>DOC_MODULE</parameter></term>
-    <listitem><para>
-    The name of the document being built
-    </para></listitem>
-  </varlistentry>
-  <varlistentry>
-    <term><parameter>DOC_ID</parameter></term>
-    <listitem><para>
-    The unique identifier for a Mallard document
-    </para></listitem>
